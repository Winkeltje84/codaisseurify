<h1>Codaisseurify</h1>
<p>Here is a list of available artists:</p>

<!-- itterate over each artist -->
<% @artists.each do |artist| %>
  <div>
    <h3><%= link_to artist.name, artist_path(artist.id) %></h3>
    <img src="<%= artist.image %>" width="200">
  </div>

  <div>
    <%= link_to "New song", edit_event_path(event), class: "btn btn-default" %>
  </div>

<% end %>
